{
  "name": "@rhinestone/1auth",
  "version": "0.1.1",
  "description": "Passkey-based authentication SDK for Web3 applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    },
    "./wagmi": {
      "types": "./dist/wagmi.d.ts",
      "import": "./dist/wagmi.mjs",
      "require": "./dist/wagmi.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhinestonewtf/1auth-sdk.git"
  },
  "homepage": "https://github.com/rhinestonewtf/1auth-sdk#readme",
  "bugs": {
    "url": "https://github.com/rhinestonewtf/1auth-sdk/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@rhinestone/sdk": "^1.2.3"
  },
  "peerDependencies": {
    "@wagmi/core": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "viem": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/react": "^18.0.0",
    "@wagmi/core": "^2.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "viem": "^2.21.0",
    "vitest": "^2.1.0"
  }
}
